var tableModel = {
options : {
title: 'Admins online:',
       pager: {
total: 100,
       pages: 1,
       currentPage: 1,
       from: 1,
       to: 100
       },
rowClass : function(rowIdx) {
		   var className = '';
		   if (rowIdx % 2 == 0) {
			   className = 'hightlight';
		   }
		   return className;
	   }

	  },
columnModel : [
	      {
id : 'ip',
     title : 'IP',
     width : 140,
